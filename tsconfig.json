{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "es6"
    ],
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "src/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
